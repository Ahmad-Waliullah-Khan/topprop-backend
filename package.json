{
    "name": "top-prop-backend",
    "version": "1.0.0",
    "description": "Project dedicated to the backed of Top Prop.",
    "keywords": [
        "loopback-application",
        "loopback"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=10.16"
    },
    "scripts": {
        "build": "npm run clean && lb-tsc",
        "build:watch": "npm run clean && lb-tsc --watch",
        "lint": "npm run prettier:check && npm run eslint",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.json\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint": "lb-eslint --report-unused-disable-directives .",
        "eslint:fix": "npm run eslint -- --fix",
        "pretest": "npm run clean && npm run build",
        "test": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__\"",
        "test:contacts": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/contact-submission.controller.acceptance.js\"",
        "test:contests": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/contest.controller.acceptance.js\"",
        "test:games": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/game.controller.acceptance.js\"",
        "test:players": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/player.controller.acceptance.js\"",
        "test:users": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/user.controller.acceptance.js\"",
        "test:teams": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/team.controller.acceptance.js\"",
        "test:wallet": "lb-mocha -r module-alias/register --bail --timeout 10000 --allow-console-logs \"dist/__tests__/acceptance/wallet.controller.acceptance.js\"",
        "posttest": "npm run prettier:fix",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
        "docker:build": "docker build -t top-prop-backend .",
        "docker:run": "docker run -p 3000:3000 -d top-prop-backend",
        "migrate": "node  -r dotenv/config -r module-alias/register ./dist/migrate",
        "openapi-spec": "node ./dist/openapi-spec",
        "prestart": "npm run build",
        "start": "node -r source-map-support/register .",
        "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
    },
    "repository": {
        "type": "git"
    },
    "author": "ivan@geekbears.com",
    "license": "",
    "files": [
        "README.md",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "_moduleAliases": {
        "@src": "dist"
    },
    "dependencies": {
        "@loopback/authentication": "^6.0.1",
        "@loopback/authorization": "^0.6.6",
        "@loopback/boot": "^2.5.1",
        "@loopback/core": "^2.9.5",
        "@loopback/cron": "^0.2.13",
        "@loopback/openapi-v3": "^3.4.9",
        "@loopback/repository": "^2.11.2",
        "@loopback/rest": "^6.2.0",
        "@loopback/rest-explorer": "^2.2.10",
        "@loopback/service-proxy": "^2.3.8",
        "bcrypt": "^5.0.0",
        "email-templates": "^7.0.5",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "loopback-connector-postgresql": "^5.0.1",
        "moment": "^2.27.0",
        "passport-facebook-token": "^4.0.0",
        "passport-google-id-token": "^0.4.7",
        "pug": "^3.0.0",
        "stripe": "^8.69.0",
        "tslib": "^2.0.0",
        "validate": "^5.1.0"
    },
    "devDependencies": {
        "@loopback/authentication-passport": "^2.1.13",
        "@loopback/build": "^6.2.2",
        "@loopback/eslint-config": "^9.0.2",
        "@loopback/express": "^1.4.1",
        "@loopback/testlab": "^3.2.4",
        "@types/bcrypt": "^3.0.0",
        "@types/cron": "^1.7.2",
        "@types/dotenv": "^8.2.0",
        "@types/email-templates": "^7.0.2",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.157",
        "@types/module-alias": "^2.0.0",
        "@types/morgan": "^1.9.1",
        "@types/node": "^10.17.28",
        "@types/passport-facebook-token": "^0.4.34",
        "@types/randomstring": "^1.1.6",
        "chalk": "^4.1.0",
        "cron": "^1.8.2",
        "dotenv": "^8.2.0",
        "eslint": "^7.7.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "randomstring": "^1.1.5",
        "source-map-support": "^0.5.19",
        "typescript": "~4.0.2"
    }
}
